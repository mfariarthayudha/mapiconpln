
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <!-- <link rel="icon" href="../images/favicon.ico"> -->

    <title>Anonim -Edit PA</title>

    <!-- Vendors Style-->
    <link rel="stylesheet" href="/sunny-admin/css/vendors_css.css" />

    <!-- Style-->
    <link rel="stylesheet" href="/sunny-admin/css/style.css" />
    <link rel="stylesheet" href="/sunny-admin/css/skin_color.css" />

    <link rel="stylesheet" href="/toastr/toastr.min.css">

    <style>
      /* CSS to style the edit button */
      .btn-edit {
        padding: 15px 5px; /* Adjust padding to change button size */
        margin: 5px 5px 5px 5px;
        height: 28px;
        width: 100%;
        font-size: 10px; /* Adjust font size */
        display: flex;
        justify-content: center;
        align-items: center;
      }

      .btn-td {
        width: 10%;
        vertical-align: middle;
        justify-content: center;
        align-items: center;
      }
    </style>
  </head>

  <body class="hold-transition light-skin sidebar-mini theme-primary">
    <div class="wrapper">
      <header class="main-header">
        <nav class="navbar navbar-static-top pl-30">
          <!-- Sidebar toggle button-->
          <div>
            <ul class="nav">
              <li class="btn-group nav-item">
                <a
                  href="#"
                  class="waves-effect waves-light nav-link rounded svg-bt-icon"
                  data-toggle="push-menu"
                  role="button"
                >
                  <i class="nav-link-icon mdi mdi-menu"></i>
                </a>
              </li>
            </ul>
          </div>

          <div class="navbar-custom-menu r-side">
            <ul class="nav navbar-nav">
              <li class="dropdown user user-menu">
                <a
                  href="#"
                  class="waves-effect waves-light rounded dropdown-toggle p-0"
                  data-toggle="dropdown"
                  title="User"
                >
                  <img src="/images/default-profile-picture.png" />
                </a>
                <ul class="dropdown-menu animated flipInX">
                  <li class="user-body">
                    <a class="dropdown-item" href="<%= `${baseUrl}/api/authentication/logout` %>"
                      ><i class="ti-lock text-muted mr-2"></i> Logout</a
                    >
                  </li>
                </ul>
              </li>
            </ul>
          </div>
        </nav>
      </header>

      <aside class="main-sidebar">
        <!-- sidebar-->
        <section class="sidebar">
          <div class="user-profile">
            <div class="ulogo">
              <a href="dashboard.html">
                <!-- logo for regular state and mobile devices -->
                <div class="d-flex align-items-center justify-content-center">
                  <!-- <img src="../images/logo-dark.png" alt=""> -->
                  <h3><b>Anonim</b> Test</h3>
                </div>
              </a>
            </div>
          </div>

          <!-- sidebar menu-->
            <ul class="sidebar-menu" data-widget="tree">
                <li>
                    <a href="<%= `${baseUrl}` %>">
                        <i data-feather="pie-chart"></i>
                        <span>Dashboard</span>
                    </a>
                </li>

                <li class="header nav-small-cap">Main Menu</li>

                <li>
                    <a href="<%= `${baseUrl}/submit-pa` %>">
                        <i data-feather="plus"></i>
                        <span>Submit PA</span>
                    </a>
                </li>

                <li class="active">
                    <a href="<%= `${baseUrl}/update-pa` %>">
                        <i data-feather="edit"></i>
                        <span>Update PA</span>
                    </a>
                </li>

                <li>
                    <a href="<%= `${baseUrl}/monitoring-pa` %>">
                        <i data-feather="database"></i>
                        <span>Monitoring PA</span>
                    </a>
                </li>

                <li>
                    <a href="<%= `${baseUrl}/aging` %>">
                        <i data-feather="pie-chart"></i>
                        <span>Aging</span>
                    </a>
                </li>

                <li>
                    <a href="<%= `${baseUrl}` %>">
                        <i data-feather="user-plus"></i>
                        <span>Daftar Akun Mitra</span>
                    </a>
                </li>
            </ul>
        </section>
      </aside>

      <!-- Content Wrapper. Contains page content -->
      <div class="content-wrapper">
        <div class="container-full">
          <!-- Content Header (Page header) -->
          <div class="content-header">
            <div class="d-flex align-items-center">
              <div class="mr-auto">
                <h3 class="page-title">Anonim App</h3>
                <div class="d-inline-block align-items-center">
                  <nav>
                    <ol class="breadcrumb">
                      <li class="breadcrumb-item">
                        <a href="dashboard.html"><i class="mdi mdi-home-outline"></i></a>
                      </li>
                      <li class="breadcrumb-item">
                     
                        <a href="<%= `${baseUrl}/update-pa` %>">Update PA</a>

                      </li>
                      <li class="breadcrumb-item active" aria-current="page">
                        <%= pa.idPa %>
                      </li>
                    </ol>
                  </nav>
                </div>
              </div>
            </div>
          </div>

          <!-- Main content -->
          <div class="box-header">
            <h4 class="box-title">Edit PA</h4>
          </div>
          <div class="box-body">
            <div class="form-group row">
              <label class="col-form-label col-lg-3">ID PA</label>
              <div class="col-lg-9">
                <input type="text" id="id-pa-input" class="form-control" value="<%= pa.idPa %>" placeholder="ID PA" disabled />
              </div>
            </div>

            <div class="form-group row">
              <label class="col-form-label col-lg-3">Customer</label>
              <div class="col-lg-9">
                <input type="text" id="customer-input" class="form-control" value="<%= pa.customer %>" placeholder="Customer" />
              </div>
            </div>


            <div class="form-group row">
              <label class="col-form-label col-lg-3">Foto Briefing K3</label>
              <div class="col-lg-9">
                <div class="custom-file">
                  <input type="file" id="foto-briefing-k3-input" class="custom-file-input">
                  <label class="custom-file-label" for="customFile">Upload Foto</label>
                </div>
              </div>
            </div>  

            <div class="form-group row">
              <label class="col-form-label col-lg-3">Panjang Tarikan (M)</label>
              <div class="col-lg-9">
                <input type="number" id="panjang-tarikan-input" class="form-control" value="<%= pa.panjangTarikan %>" placeholder="Panjang Tarikan (M)" />
              </div>
            </div>

            <div class="form-group row">
              <label class="col-form-label col-lg-3">Progres Tarikan (M)</label>
              <div class="col-lg-9">
                <input type="number" id="progres-tarikan-input" class="form-control" value="<%= pa.progresTarikan %>" placeholder="Progres Tarikan (M)" />
              </div>
            </div>

            <div class="form-group row">
              <label class="col-form-label col-lg-3">Jumlah JB</label>
              <div class="col-lg-9">
                <input type="number" id="jumlah-jb-input" class="form-control" value="<%= pa.jumlahJb %>" placeholder="Jumlah JB" />
              </div>
            </div>

            <div class="form-group row">
                <label class="col-form-label col-lg-3">Tracing Core</label>
                <div class="col-lg-9">
                  <select id="tracing-core-input" class="form-control">
                    <option value="0" <%= pa.tracingCore == 0 ? 'selected' : '' %>>0%</option>
                    <option value="50" <%= pa.tracingCore == 50 ? 'selected' : '' %>>50%</option>
                    <option value="100" <%= pa.tracingCore == 100 ? 'selected' : '' %>>100%</option>
                  </select>
                </div>
              </div>

            <div class="form-group row">
              <label class="col-form-label col-lg-3">Testcom</label>
              <div class="col-lg-9">
                <select id="testcom-input" class="form-control">
                  <option value="0" <%= pa.testcom == 0 ? 'selected' : '' %>>0%</option>
                  <option value="50" <%= pa.testcom == 50 ? 'selected' : '' %>>50%</option>
                  <option value="100" <%= pa.testcom == 100 ? 'selected' : '' %>>100%</option>
                </select>
              </div>
            </div>

            <div class="form-group row">              
              <label class="col-form-label col-lg-3">Upload Tescom</label>
              <div class="col-lg-9">
                <div class="custom-file">
                  <input type="file" id="file-testcom-input" class="custom-file-input">
                  <label class="custom-file-label" for="customFile">Upload Tescom</label>
                </div>
              </div>
            </div>  

            <div class="form-group row">
              <label class="col-form-label col-lg-3">BAI User</label>
              <div class="col-lg-9">
                <select id="bai-user-input" class="form-control">
                    <option value="0" <%= pa.baiUser == 0 ? 'selected' : '' %>>0%</option>
                    <option value="50" <%= pa.baiUser == 50 ? 'selected' : '' %>>50%</option>
                    <option value="100" <%= pa.baiUser == 100 ? 'selected' : '' %>>100%</option>
                </select>
              </div>
            </div>

          

            <div class="form-group row">
              <label class="col-form-label col-lg-3">Upload BAI BAKL</label>
              <div class="col-lg-9">
                <div class="custom-file">
                  <input type="file" id="file-bai-bakl-input" class="custom-file-input">
                  <label class="custom-file-label" for="customFile">Upload BAI</label>
                </div>
              </div>
            </div>  

            <div class="form-group row">
              <label class="col-form-label col-lg-3">Tanggal BAI</label>
              <div class="col-lg-9">
                <input type="date" id="tanggal-bai-input" class="form-control" value="<%= pa.tanggalBai %>" />

              </div>
            </div>

            <div class="form-group row">
              <label class="col-form-label col-lg-3">Kendala</label>
              <div class="col-lg-9">
                <textarea id="kendala-input" rows="5" cols="5" class="form-control" placeholder="Kendala"><%= pa.kendala %></textarea>
              </div>
            </div>

            <div class="clearfix pull-right">
              <a type="button" id="update-button" class="btn btn-rounded btn-primary mb-5">Update</a>
            </div>
          </div>

         
          <!-- /.content -->
        </div>
      </div>
      <!-- /.content-wrapper -->
    </div>
    <!-- ./wrapper -->

    <!-- Vendor JS -->
    <script src="/sunny-admin/js/vendors.min.js"></script>
    <script src="/sunny-admin/assets/icons/feather-icons/feather.min.js"></script>
    <script src="/sunny-admin/assets/vendor_components/datatable/datatables.min.js"></script>
    <script src="/sunny-admin/js/pages/data-table.js"></script>
    <script src="/sunny-admin/js/pages/advanced-form-element.js"></script>

    <!-- Sunny Admin App -->
    <script src="/sunny-admin/js/template.js"></script>

    <script src="/toastr/toastr.min.js"></script>

    <script src="/axios/axios.min.js"></script>
    <script src="/jquery/jquery.min.js"></script>

    <script src="/javascript/main.js"></script>
    <script src="/javascript/form-update-pa.js"></script>
  </body>
</html>
